@model ElectronicsAppMVC.Models.Student
@{
    ViewData["Title"] = "Register1";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Register1</h1>
@*anchor tag helper*@
<a asp-controller="Student" asp-action="Index">Student Details</a>
@*form tag helper*@
<form asp-action="Register">
    <div class="form-group">
    @*label tag helper*@
    @Html.Label("Id","Student Id", new { @class="control-label"})
    @Html.LabelFor(model => model.Id, new { @class="control-label"})
    <label asp-for="Id" class="control-label"></label>
    @*input tag helper*@
    <input asp-for="Id" class="form-control"    />
    @*validation tag helper*@
    <span asp-validation-for="Id" class="text-danger"></span>
    @*textarea tag helper*@
   <textarea asp-for="Address"></textarea>
</div>
</form>


